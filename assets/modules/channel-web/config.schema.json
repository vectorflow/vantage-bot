      "default": false,
      "type": "boolean"
    },
    "uploadsS3Bucket": {
      "default": "bucket-name",
      "type": "string"
    },
    "uploadsS3Region": {
      "default": "eu-west-1",
      "type": "string"
    },
    "uploadsS3AWSAccessKey": {
      "default": "your-aws-key-name",
      "type": "string"
    },
    "uploadsS3AWSAccessSecret": {
      "default": "secret-key",
      "type": "string"
    },
    "startNewConvoOnTimeout": {
      "default": false,
      "type": "boolean"
    },
    "recentConversationLifetime": {
      "default": "6 hours",
      "type": "string"
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}

